T () -> F ();
=================================
fix f0.fun x0. ( False )
0.0002 false

F () -> T ();
=================================
fix f0.fun x0. ( True )
0.0002 false

T () -> F ();
F () -> T ();
=================================
fix f0.fun x0.
  match (x0) with
  | T -> ( False )
  | F -> ( True )
  
0.0002 true

