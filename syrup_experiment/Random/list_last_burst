fix (f : list -> natopt) =
  fun (x:list) ->
    match x with
      | Nil _ -> None
      | Cons _ -> (match f (Un_Cons x . 1) with
                     | None _ -> Some (Un_Cons x . 0)
                     | Some _ -> f (Un_Cons x . 1))

